@page
@model Compositions.Pages.CreateModel
@{
    ViewData["Title"] = "Создать музыку";
}
<h1>Create Music</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Music.Attributes"></label>
        <input asp-for="Music.Attributes" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Music.SongTitle"></label>
        <input asp-for="Music.SongTitle" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Music.Artist"></label>
        <input asp-for="Music.Artist" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Music.MusicAuthors"></label>
        <input asp-for="Music.MusicAuthors" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Music.TextAuthors"></label>
        <input asp-for="Music.TextAuthors" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Music.Rating"></label>
        <input asp-for="Music.Rating" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>

    <button type="submit" class="btn btn-primary">Back</button>
</form>
